<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.seoulapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SearchActivity"></activity>
        <activity
            android:name=".BottomNavigation"
            android:label="@string/title_activity_bottom_navigation" />
        <activity android:name=".ShopDetaildInfo" />
        <activity android:name=".Signup" />
        <activity android:name=".ui.notifications.NewsActivity" />
        <activity android:name=".ui.notifications.SettingsActivity" />
        <activity android:name=".ui.notifications.EditProfileActivity" />
        <activity android:name=".ImageActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>